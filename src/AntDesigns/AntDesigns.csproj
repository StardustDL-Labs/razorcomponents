<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RootNamespace>StardustDL.RazorComponents.AntDesigns</RootNamespace>
    <AssemblyName>StardustDL.RazorComponents.AntDesigns</AssemblyName>
    <Nullable>enable</Nullable>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Authors>StardustDL</Authors>
    <Company></Company>
    <Copyright>StardustDL</Copyright>
    <Description>Wrapper for AntDesign Blazor at https://github.com/ant-design-blazor/ant-design-blazor.</Description>
    <PackageProjectUrl>https://github.com/StardustDL/razorcomponents</PackageProjectUrl>
    <RepositoryUrl>https://github.com/StardustDL/razorcomponents</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>blazor;component-library;razor-components;razor;ant-design</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AntDesign" Version="0.8.1" />
    <PackageReference Include="Modulight.Modules.Client.RazorComponents" Version="0.0.6" />
  </ItemGroup>

</Project>
