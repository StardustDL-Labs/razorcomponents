<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Nullable>enable</Nullable>
    <Authors>StardustDL</Authors>
    <Copyright>StardustDL</Copyright>
    <PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/StardustDL/razorcomponents</PackageProjectUrl>
    <RepositoryUrl>https://github.com/StardustDL/razorcomponents</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>StardustDL.RazorComponents.MaterialDesignIcons</PackageId>
    <Product>StardustDL.RazorComponents.MaterialDesignIcons</Product>
    <Description>Material Design Icons for Blazor, Razor Pages and so on from https://materialdesignicons.com/ .</Description>
    <RootNamespace>StardustDL.RazorComponents.MaterialDesignIcons</RootNamespace>
    <AssemblyName>StardustDL.RazorComponents.MaterialDesignIcons</AssemblyName>
    <PackageTags>blazor;component-library;razor-components;icons;google-material;material-design;webfonts;razor</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="package-lock.json" />
    <Content Remove="package.json" />
  </ItemGroup>

  <ItemGroup>
    <None Include="package-lock.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </None>
    <None Include="package.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Modulight.Modules.Client.RazorComponents" Version="0.0.7" />
  </ItemGroup>
</Project>
